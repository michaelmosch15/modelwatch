{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run: GPT-2 generate (CPU)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/app/scripts/generate_gpt2.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Run: app/main.py (rewrite sample 0:5)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/app/main.py",
      "console": "integratedTerminal",
      "args": [
        "rewrite",
        "--input", "${workspaceFolder}/app/data/sample_inputs.csv",
        "--output", "${workspaceFolder}/app/data/out_rewrite.csv",
        "--start", "0",
        "--end", "5"
      ]
    },
    {
      "name": "Run: app/main.py (detect PII body)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/app/main.py",
      "console": "integratedTerminal",
      "args": [
        "detect",
        "--input", "${workspaceFolder}/app/data/sample_inputs.csv",
        "--output", "${workspaceFolder}/app/data/out_detect.csv"
      ]
    },
    {
      "name": "Run: app/main.py (anonymize body)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/app/main.py",
      "console": "integratedTerminal",
      "args": [
        "anonymize",
        "--input", "${workspaceFolder}/app/data/sample_inputs.csv",
        "--output", "${workspaceFolder}/app/data/out_anon.csv"
      ]
    },
    {
      "name": "Run: Apply PII Layer (anonymize body)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/app/scripts/apply_pii_layer_csv.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Run: Detect PII in emails (body)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/app/scripts/detect_pii_csv.py",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
